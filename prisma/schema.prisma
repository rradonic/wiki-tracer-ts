generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../data/wiki-tracer-ts.db"
}

model Page {
  id         Int    @id @default(autoincrement())
  title      String @unique
  linkedFrom Page[] @relation("PageLink")
  linksTo    Page[] @relation("PageLink")
}
